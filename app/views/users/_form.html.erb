<%= form_with model: @user do |form| %>
  <%= form.label :age %>
  <%= form.text_field :age %>
  <%= form.label :weight %>
  <%= form.text_field :weight %>
  <%= form.label :height %>
  <%= form.text_field :height %>
  <%= form.label :kilos %>
  <%= form.text_field :kilos_to_lose %>

  <%= form.submit %>
<% end %>





